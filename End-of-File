import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        for(int i=1;s.hasNext()==true;i++)
        {
        String str=s.nextLine();

        System.out.println(i+" "+str);
        
        }
        s.close();
        
    }
}
